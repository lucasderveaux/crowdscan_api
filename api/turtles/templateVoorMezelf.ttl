@prefix rdf: <http://wwww.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <https://www.w3.org/2001/XMLSchema#> .
@prefix time: <http://www.w3.org/2006/time#> .

<crowdscanDrukte> rdf:type sosa:FeatureOfInterest ;
  rdfs:label "Drukte in de Langemunt in Gent" .

<langemunt> a sosa:platform ;
  rdfs:comment "de lange munt heeft 3 delen waar sensoren geplaatst zijn" ;
  sosa:hosts <groentemarkt>, <gent_langemunt1>, <gent_langemunt2> .

<groentemarkt> a sosa:sensor ;
  rdfs:comment "De sensor in het gebied van de Groentemarkt";
  sosa:isHostedBy <langemunt> .

<gent_langemunt1> a sosa:sensor ;
  rdfs:comment "De sensor in het gebied Langemunt deel 1" ;
  sosa:isHostedBy <langemunt> .

<gent_langemunt2> a sosa:sensor ;
  rdfs:comment "De sensor in het gebied Langemunt deel 2" ;
  sosa:isHostedBy <langemunt> .

<averageHeadCount> a sosa:ObservableProperty ;
  rdfs:label "Average amount of people within a certain timeframe" .

<observation1> a sosa:Observation ;
  sosa:hasFeatureOfInterest <crowdscanDrukte> ;
  sosa:observedProperty <averageHeadCount> ;
	sosa:resultTime "2021-06-22T12:37:36.495285"^^xsd:dateTime ;
	sosa:hasSimpleResult "33.585004325759584"^^xsd:double ;
  sosa:phenomenonTime [
    rdf:type time:Interval ;
    time:minutes 3
  ] ;
	sosa:madeBySensor <groentemarkt> .

<observation2> a sosa:Observation ;
  sosa:hasFeatureOfInterest <crowdscanDrukte> ;
  sosa:observerdProperty <averageHeadCount> ;
	sosa:resultTime "2021-06-22T12:37:36.495285"^^xsd:dateTime ;
	sosa:hasSimpleResult "38.875591201835434"^^xsd:double ;
  sosa:phenomenonTime [
    rdf:type time:Interval ;
    time:minutes 3
  ];
	sosa:madeBySensor <gent_langemunt1> .

<observation3> a sosa:Observation ;
  sosa:hasFeatureOfInterest <crowdscanDrukte> ;
  sosa:observerdProperty <averageHeadCount> ;
	sosa:resultTime "2021-06-22T12:37:36.495285"^^xsd:dateTime ;
	sosa:hasSimpleResult "1.1199560146372134"^^xsd:double ;
  sosa:phenomenonTime [
    rdf:type time:Interval ;
    time:minutes 3
  ] ;
	sosa:madeBySensor <gent_langemunt2> .